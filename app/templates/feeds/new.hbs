{{#paper-toolbar className="ember-app"}}
  <div class="md-toolbar-tools">
    <h2 flex=true class="md-toolbar-tools">
      {{#paper-sidenav-toggle class="menu-sidenav-toggle"}}
        {{paper-icon icon="menu"}}
      {{/paper-sidenav-toggle}}
      <img src="/images/emberjs-logo.png" height="50">
      <strong>New Feed</strong>
    </h2>
  </div>
{{/paper-toolbar}}
{{#paper-content}}
  <div class="container">
    {{paper-text label="Name" value=name}}
    {{paper-text label="URL" value=url}}
    {{#paper-button raised=true classNames="md-primary" action="save" }}
      Save
    {{/paper-button}}
    &nbsp;
    {{#paper-button raised=true action="cancel" }}
      Cancel
    {{/paper-button}}
  </div>
{{/paper-content}}
